{
  "name": "template-nuxt",
  "private": true,
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "start": "node .output/server/index.mjs",
    "preview": "nuxt preview",
    "test": "vitest",
    "test:playwright": "playwright install && playwright test",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "simple-git-hooks",
    "postinstall": "nuxt prepare",
    "storybook": "storybook dev -p 6006 --no-open"
  },
  "dependencies": {
    "@nuxtjs/i18n": "^8.3.1",
    "@pinia/nuxt": "~0.5.1",
    "@sit-onyx/icons": "0.1.0-alpha.2",
    "@sit-onyx/nuxt": "1.0.0-alpha.15",
    "@vue/server-renderer": "^3.4.28",
    "@vueuse/core": "^10.11.0",
    "canvas-confetti": "^1.9.3",
    "engine.io": "^6.5.4",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "sit-onyx": "1.0.0-alpha.135",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "vue": "^3.4.28",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@nuxt/eslint": "~0.3.13",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/storybook": "^8.1.1",
    "@playwright/test": "~1.44.1",
    "@sit-onyx/storybook-utils": "1.0.0-alpha.140",
    "@storybook-vue/nuxt": "~0.2.7",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/vue3": "^8.1.9",
    "@types/canvas-confetti": "^1.6.4",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-playwright": "^1.6.2",
    "happy-dom": "^14.12.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "sass": "^1.77.5",
    "simple-git-hooks": "^2.11.1",
    "storybook": "^8.1.9",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  }
}
